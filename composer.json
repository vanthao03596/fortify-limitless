{
    "name": "vanthao03596/fortify-limitless",
    "description": "Fortify-driven Laravel UI replacement",
    "keywords": [
        "vanthao03596",
        "laravel",
        "fortify-limitless"
    ],
    "homepage": "https://github.com/vanthao03596/fortify-limitless",
    "license": "MIT",
    "authors": [
        {
            "name": "phamthao",
            "email": "phamthao03596@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Vanthao03596\\FortifyLimitless\\": "src",
            "Vanthao03596\\FortifyLimitless\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanthao03596\\FortifyLimitless\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vanthao03596\\FortifyLimitless\\FortifyLimitlessServiceProvider"
            ],
            "aliases": {
                "FortifyLimitless": "Vanthao03596\\FortifyLimitless\\FortifyLimitlessFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
